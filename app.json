{
  "expo": {
    "name": "Ordee",
    "slug": "pos-restaurant",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/logoFinal.png",
    "scheme": "pos-restaurant",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logoFinal.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoFinal.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.miguelparis.posrestaurant"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Ordee tendra acceso a tus fotos para poder agregarlas a tus menues."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "28172c6c-3c04-4eb4-a1ca-58aacde4691d"
      }
    }
  }
}
